{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-intl": "*",
        "ext-json": "*",
        "aws/aws-sdk-php-laravel": "^3.4",
        "barryvdh/laravel-cors": "^0.11.4",
        "coreproc/laravel-notification-channel-fcm": "^1.5",
        "fideloper/proxy": "^4.0",
        "laravel-notification-channels/webpush": "^5.0",
        "laravel/framework": "^6.2",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/telescope": "3.*",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "myclabs/php-enum": "^1.7",
        "propaganistas/laravel-phone": "^4.2",
        "stripe/stripe-php": "^7.12",
        "tymon/jwt-auth": "^1.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-query-detector": "^1.1",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.4.3",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpcs": [
            "./vendor/bin/phpcs --standard=phpcs.xml ./"
        ],
        "phpcbf": [
            "./vendor/bin/phpcbf --standard=phpcs.xml ./"
        ],
        "phpstan": [
            "php -d memory_limit=1G artisan code:analyse"
        ],
        "phpunit": [
            "php -d memory_limit=1G ./vendor/bin/phpunit"
        ]
    }
}
